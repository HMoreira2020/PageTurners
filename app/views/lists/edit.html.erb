<div class="title-container">
<h1>Edit Your List</h1>
</div>
<%= render "errors/error", model: @list %>
<%= form_for @list do |f| %> 
  <div class="form-group">
  <%= f.label :title, "List Title" %> 
  <%= f.text_field :title, class: "form-control" %><br><br>
  </div>
  <%= f.submit class: "btn btn-primary"%>
<%end%>

<%= form_tag list_path(@list), method: "delete" do %>
    <%= submit_tag "Delete This List", class: "btn btn-primary" %>
<% end %>

  

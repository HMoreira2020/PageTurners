<h1>Your lists</h1>
<% if @user.lists.empty? %>
    You have no lists yet.
    <%= link_to "Create a Book List", new_user_list_path %>
<%else%>
 <%= @lists.each do |list| %> 
    <h2><%= list.title %></h2>
    <h3>by: <%= link_to list.user.name, user_lists_path(list.user) %></h3>
        <%= list.books.each do |book| %>
        <%= link_to book.title, book_path(book) %> by <%= book.author %> 
        
<%end%>
<%end%>
<%end%>

  